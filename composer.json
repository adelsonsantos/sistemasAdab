{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "host33/yii2-multilevel-vertical-menu": "dev-master",
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "kint-php/kint": "~2.0.0",
        "zyx/zyx-phpmailer": "~0.9.2",
        "kartik-v/yii2-dialog": "*",
        "twbs/bootstrap": "^4.0",
        "zytzagoo/smtp-validate-email": "*"
    },
    "require-dev": {
        "2amigos/yii2-date-picker-widget" : "~1.0.7",
        "quickemailverification/quickemailverification": "*",
        "symfony/dom-crawler": "~2.8",
        "symfony/css-selector": "~2.8",
        "wbraganca/yii2-dynamicform": "~2.0.2",
        "kartik-v/yii2-tabs-x": "~1.2.3",
        "kartik-v/yii2-nav-x": "~1.2.1",
        "cinghie/yii2-menu": "~0.8.1",
        "tcdent/php-restclient": "~0.1.6",
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",yii2-dynamicform
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3",
        "kartik-v/yii2-dynagrid": "~1.4.5",
        "kartik-v/yii2-widget-timepicker": "*",
        "fxp/composer-asset-plugin":"~1.3",
        "mouf/nodejs-installer": "~1.0",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-krajee-base": "*",
        "bower-asset/vue": "~2.3.0",
        "bower-asset/vue-resource": "*",
        "bower-asset/vee-validate": "~2.0.0",
        "bower-asset/vue-components-smartadmin": "*",
        "bower-asset/vuex": "~2.3.0",
        "bower-asset/vue-router": "~2.5.3",
        "aki/yii2-vue": "*",
        "mpdf/mpdf": "~7.0.0",
        "kartik-v/yii2-widget-activeform": "*",
        "kartik-v/yii2-widget-select2": "~2.1.1",
        "ddtraceweb/smtp-validator-email": "*",
        "yiisoft/yii2-jui": "~2.0.0"
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        },
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
